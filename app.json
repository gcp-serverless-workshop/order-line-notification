{
  "name": "skatepark-line",
  "env": {
      "LINE_CHANNEL_ACCESS_TOKEN": {
          "description": "LINE Bot access token",
          "value": "ooooo",
          "required": true
      },
      "LINE_CHANNEL_SECRET": {
        "description": "LINE Bot secret",
        "value": "xxxx",
        "required": true
      },
      "API_ENV": {
        "description": "API environment",
        "value": "production",
        "required": true
      },
      "LOG": {
        "description": "Log level",
        "value": "INFO",
        "required": true
      }
  },
  "options": {
      "allow-unauthenticated": true,
      "memory": "256Mi",
      "cpu": "1",
      "port": 8080,
      "http2": false,
      "concurrency": 5,
      "max-instances": 2
  }
}